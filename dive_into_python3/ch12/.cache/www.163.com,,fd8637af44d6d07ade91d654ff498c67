(function() {
    var nav_value = {
        platform : ""
    };
    var nav = {
        detect : function() {
            if (navigator.platform.indexOf("Win") > -1) {
                nav_value.platform = "ua-win";
            } else if (navigator.platform.indexOf("Mac") > -1) {
                nav_value.platform = "ua-mac";
            } else if (navigator.platform.indexOf("iPad") > -1 || navigator.platform.indexOf("iPhone") > -1) {
                nav_value.platform = "ua-ios";
            } else {
                return false
            }
            ;
        },
        init : function() {
            this.detect();
            if (nav_value.platform) {
                $("html").addCss(nav_value.platform);
            };
        }
    };
    nav.init();
})();
NTES.ready(function(){
    var App = NTESCommonNavigator.core.Controller,
            Event = NTESCommonNavigator.core.Event.Base,
            ModelEvent = NTESCommonNavigator.core.Event.Model,
            app = App();
    app.bind(Event.DATA_CHANGED, function(data){
    });
    app.bind(ModelEvent.MAIL_INFO_LOADED, function(data){
        if($("#js_N_navMailMsg").style.display == "block"){
            $("#js_mail_url").addEvent("click",function(){
                $("#js_N_navMailMsg").style.display = "none";
            })
        }
    });
    app.bind(ModelEvent.WEIBO_INFO_LOADED, function(data){
        if($("#js_N_navWbMsg").style.display == "block"){
            $($("#js_N_navWbMsg").parentNode).addEvent("click",function(){
                $("#js_N_navWbMsg").style.display = "none";
            })
        }
    });
    app.refresh();
});
//yodao keyword weather 
(function () {
    var ydConfig = {
        ipQuery: "http://ip.ws.126.net/ipquery",
        dataCity: "http://www.163.com/special/0077450P/citycode.html",
        dataUrl: "http://www.163.com/inc/163new/youdao/tq_id_",
        cityCode: "",
        cityName: "",
        defaultCity: "北京",
        defaultCode: "54511",
        keywordUrl: "http://www.163.com/inc/163new/youdao/hw.html",
        AutoCompleteJs:"http://img3.cache.netease.com/www/v2013/js/autocomplete_v1.js"
    };
    var ydService = {
        init: function(){
            this.initSearch();
        },
        initSearch:function(){
            var that=this;
            $("#ydQuery").addEvent("mouseover", function(e){
                if(e.target.className=="youdao-search-input" && !that.searchLock){
                    that.searchLock=true;
                    NTES.ajax.importJs(ydConfig.AutoCompleteJs, function(){
                        if(typeof(AutoComplete) == 'function'){
                            aa = new AutoComplete("query", "ydQuery","aa", true, false, "", true, "http://www.youdao.com");
                            aa.setSearchServer("http://www.youdao.com/search?");
                            aa.setLogServer("http://www.youdao.com/");
                            aa.setKeyFrom("163.index");
                            aa.showContent = function(){
                                var inputbox = document.getElementById("query");
                                var position = SP.cumOffset(inputbox);
                                this.sdiv.style.top = position[1] + inputbox.offsetHeight + "px";
                                if (this.bName == "IE" && this.bVer == 8) {
                                    this.sdiv.style.left = (position[0] - 1) + "px";
                                } else {
                                    this.sdiv.style.left = (position[0] + 1) + "px";
                                }
                                this.sdiv.style.cursor = "default";
                                this.sdiv.style.width = (inputbox.offsetWidth + 1) + "px";
                                SElement.show(this.sdiv);
                                this.vis = true;
                                this.curNodeIdx = -1;
                            }
                            aa.start();
                        }
                    });
                }
            });
            $(".youdao-search-select").addEvent("click",function(e){
                e.preventDefault();
                e.cancelBubble=true;
                $(".search-select-list")[0].style.display = $(".search-select-list")[0].style.display=="block" ? "none" : "block";
                if($(".search-select-list")[0].style.display == "block"){
                    document.onclick = function(){
                        $(".search-select-list")[0].style.display = "none";
                        $(".youdao-search-select").removeCss("youdao-search-active");
                    }
                    $(".youdao-search-select").addCss("youdao-search-active");
                }else{
                    $(".youdao-search-select").removeCss("youdao-search-active");
                }
            });
            $(".search-select-list li a").each(function(index,item){
                $(item).addEvent("click",function(e){
                    e.preventDefault();
                    e.cancelBubble=true;
                    $(".search-select-name")[0].innerHTML=item.innerHTML;
                    $(".search-select-list")[0].style.display="none";
                    $(".youdao-search-select").removeCss("youdao-search-active");
                });
            });
            //tag search
            $("#ydSubmit").addEvent("click", function(e){
                e.preventDefault();
                var product;
                switch($(".search-select-name")[0].innerHTML){
                    case "网页":product="www";break;
                    case "图片":product="image";break;
                    case "惠惠":product="huihui";break;
                    case "视频":product="video";break;
                    case "热闻":product="news";break;
                    case "词典":product="dict";break;
                    case "翻译":product="fanyi";break;
                    case "笔记":product="note";break;
                    case "更多":product="more";break;
                }
                that.changeProduct(product);
            });
        },
        getSearchUrl:function(inputId, product, keyfrom){
            var url = "http://" + product + ".youdao.com/";
            if (window.RegExp && window.encodeURIComponent) {
                var input = document.getElementById(inputId);
                var query = input.value;
                if (query != "") {
                    query = query.replace(/(^link:)|(^inlink:)|(^related:)/,"");
                    if(product=="fanyi"){
                        url = url + "translate?i=" + encodeURIComponent(query) + "&keyfrom=" + keyfrom;
                    }else if(product=="more"){
                        url="http://www.youdao.com/about/productlist.html";
                    }else if(product=="huihui"){
                        url="http://www.huihui.cn/search?q="+query;
                    }else if(product=="note"){
                        url="http://note.youdao.com";
                    }else{
                        url = url + "search?q=" + encodeURIComponent(query) + "&keyfrom=" + keyfrom;
                    }
                } else {
                    url = url + "?keyfrom=" + keyfrom;
                }
            }
            return url;
        },
        changeProduct:function(p){
            var url = this.getSearchUrl("query", p, "163.index");
            window.open(url, "_blank");
        }
    };
    ydService.init();
})();
function isYdDefault(s){return false}
function ydInputFocus(e){return false}
function ydInputBlur(e){}
