    var House = {
        setLocalCity:function(locCode){
            var i = -1,name_1,url_1,url_2,url_2_2,url2_2_name,url_3,loadContent1,loadContent2,loadContent3;
            while(++i < HouseConfig.vars.cityLen){
                if(locCode == HouseConfig.city[i].str){
                    HouseConfig.city[i].name.length > 2 ? name_1 = HouseConfig.city[i].name : name_1 = HouseConfig.city[i].name + "房产";
                    url_1 = HouseConfig.city[i].url1;
                    url2_2_name = HouseConfig.city[i].url2_2_name;
                    HouseConfig.city[i].url2 ? url_2 = HouseConfig.city[i].url2 : url_2 = HouseConfig.city[0].url2;
                    url_2_2 = HouseConfig.city[i].url2_2;
                    HouseConfig.city[i].url3 ? url_3 = HouseConfig.city[i].url3 : url_3 = HouseConfig.city[0].url3;
                    loadContent1 = HouseConfig.city[i].src1;
                    HouseConfig.city[i].src2 ? loadContent2 = HouseConfig.city[i].src2 :  loadContent2 = HouseConfig.city[0].src2;
                    HouseConfig.city[i].src3 ? loadContent3 = HouseConfig.city[i].src3 :  loadContent3 = HouseConfig.city[0].src3;
                }
            }
            HouseConfig.vars.tab_1.innerHTML = name_1;
            HouseConfig.vars.tab_1.href = url_1;
            HouseConfig.vars.tab_2.href = url_2;
            HouseConfig.vars.tab_2_2.innerHTML = url2_2_name;
            HouseConfig.vars.tab_2_2.href = url_2_2;
            HouseConfig.vars.tab_3.href = url_3;
            NTES.ajax.send(loadContent1, "GET", null, {
                onSuccess: function(xhr){
                    HouseConfig.vars.content_1.innerHTML = xhr.responseText;
                }
            });
            NTES.ajax.send(loadContent2, "GET", null, {
                onSuccess: function(xhr){
                    HouseConfig.vars.content_2.innerHTML = xhr.responseText;
                    (function(){
                        var scrollWraps = $(".H-house-scrollBox");
                        scrollWraps.each(function(i, elm){
                            elm = $(elm);
                            var currentNum = $(".house-numCount", elm).get(0);
                            new NTES.ui.Carousel(null, $(".H-house-scrollbBody li", elm), "current", {
                                prev : $(".house-scrlPrev", elm),
                                next : $(".house-scrlNext", elm),
                                event : "click",
                                interval : 5000,
                                duration : 2000,
                                auto : false,
                                onSwitch: function(i, count){
                                    i = parseInt(i);
                                    i = i || 0;
                                    currentNum.innerHTML = parseInt(i);
                                }
                            });
                        });
                    })();
                }
            });
            NTES.ajax.send(loadContent3, "GET", null, {
                onSuccess: function(xhr){
                    HouseConfig.vars.content_3.innerHTML = xhr.responseText;
                }
            });
        },
        init:function(){
            HouseConfig.vars.cityLen = HouseConfig.city.length;
            HouseConfig.vars.provinceLen = HouseConfig.province.length;
            //select current city
            var selectcitys = $("#H-house-selectcity li");
            selectcitys.addEvent("mouseover",function(e){
                this.style.background = "#333333";
                this.style.color = "#ffffff";
            });
            selectcitys.addEvent("mouseout",function(e){
                this.style.background = "#f7f7f7";
                this.style.color = "#666666";
            });
            selectcitys.addEvent("click",function(e){
                var str = $(this).getAttribute("d_code");
                House.setLocalCity(str);
                $("#H-house-selectcity").style.display = "none";
            });
            //end
            NTES.ajax.importJs(HouseConfig.ip, function(){
                var locStr = "";
                var j = -1;
                while (++j < HouseConfig.vars.cityLen) {
                    if (localAddress.city.indexOf(HouseConfig.city[j].name) != -1) {
                        locStr = HouseConfig.city[j].str;
                    }
                }
                if(!locStr){
                    var i = -1;
                    while(++i < HouseConfig.vars.provinceLen){
                        if(HouseConfig.province[i].name.indexOf(localAddress.province) != -1){
                            locStr = HouseConfig.province[i].str;
                        }
                    }
                }
                if (!locStr) {
                    locStr = HouseConfig.defaultCity.str;
                }
                House.setLocalCity(locStr);
            });
            //end
            //买房跳转
            var BuyHouseConfig = {
        city: [{  
             name: "广州", 
             url:"http://xf.house.163.com/gz/",
             str:"gz" 
			 },{  
             name: "北京", 
             url:"http://xf.house.163.com/bj/",
             str:"bj" 
			 },{  
             name: "上海", 
             url:"http://xf.house.163.com/sh/",
             str:"sh" 
			 },{  
             name: "河北", 
             url:"http://xf.house.163.com/sjz/",
             str:"sjz" 
			 },{  
             name: "福建", 
             url:"http://fj.house.163.com/xf",
             str:"fj" 
			 },{  
             name: "唐山", 
             url:"http://ts.house.163.com/xf",
             str:"ts" 
			 },{  
             name: "合肥", 
             url:"http://hf.house.163.com/xf",
             str:"hf" 
			 },{  
             name: "南宁", 
             url:"http://gx.house.163.com/xf",
             str:"gx" 
			 },{  
             name: "南京", 
             url:"http://nj.house.163.com/xf",
             str:"nj" 
			 }
        ],
        province: [{
            name: "北京市",
            str:"bj"
        }, {
            name: "上海市",
            str:"sh"
        }, {
            name: "河北省",
            str:"sjz"
        }, {
            name: "安徽省",
            str:"hf"
        }, {
            name: "福建省",
            str:"fj"
        }, {
            name: "广西",
            str:"gx"
        }, {
            name: "江苏省",
            str:"nj"
        }],
        defaultCity: {//默认为广州
            str:"gz"
        }
	}
	var SetLocalBuyHouse = function(str){
		var len = BuyHouseConfig.city.length;
		var url;
		var i = -1;
		while(++i < len){
			if(BuyHouseConfig.city[i].str == str){
				url = BuyHouseConfig.city[i].url;
				$("#buyHouse").href = url;
			}
		}
	}
NTES.ajax.importJs(HouseConfig.ip, function(){
	var clen = BuyHouseConfig.city.length , plen = BuyHouseConfig.province.length;
	var locStr = "";
    var j = -1;
    while (++j < clen) {
        if (localAddress.city.indexOf(BuyHouseConfig.city[j].name) != -1) {
           locStr = BuyHouseConfig.city[j].str;
        }
    }
    if(!locStr){
       var i = -1;
            while(++i < plen){
                  if(BuyHouseConfig.province[i].name.indexOf(localAddress.province) != -1){
                       locStr = BuyHouseConfig.province[i].str;
                   }
             }
     }
    if (!locStr) {
       locStr = BuyHouseConfig.defaultCity.str;
    }
	SetLocalBuyHouse(locStr);
});
            //end
        }
    }
    House.init();
})();
//end   house
//auto
(function(){
    if(NTES("#js-autopro-search")){
        var searchBox = NTES("#js-autopro-search"),
                searchTx = NTES.one(".autopro-search-tx",searchBox),
                searchBtn = NTES.one(".autopro-search-btn",searchBox),
                sDefault = searchTx.getAttribute("_sdefaultvalue"),
                slUrl = "http://product.auto.163.com/autosearch/pv-0-0-2_ff-8_ff-0-0-0-0-0-1.html";
        searchTx.value = sDefault;
        searchTx.addEvent("focus",function(){
            if(searchTx.value == sDefault){
                searchTx.value = "";
            }
        }).addEvent("blur",function(){
                    if(searchTx.value == ""){
                        searchTx.value = sDefault;
                    }
                })
        searchBtn.onclick = function(){
            var searchkey = searchTx.value.replace(/^\s+/, '').replace(/\s+$/, '');
            if (searchkey == '' || searchkey == sDefault) { alert("请输入要查询的内容"); return;}
            else{
                url = slUrl + "?q=" + encodeURIComponent(encodeURIComponent(searchkey)) +"#YS1001";
                window.open(url);
            }
        }
    }
})();
