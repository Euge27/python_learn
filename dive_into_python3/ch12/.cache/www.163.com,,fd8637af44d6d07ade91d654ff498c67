<script type="text/javascript">
                    (function(){
                        var api_interest = "http://api.pr.163.com/ad/www?callback=recommendad&boardid=12760";
                        window.recommendad  =   function(data){
                            adlinks =   data;
                        };
                        NTES.ajax.importJs(api_interest,function(){
                            var len_max = 4,len_li = adlinks.length/2;
                            if(adlinks != '' && len_li <= len_max && NTES('js_interst_ad')){
                                var html = [],snippt = '';
                                for(var i=j=0,k=1;i<len_li;i++,j=j+2,k=j+1){
                                    snippt = '<li><a href="' + adlinks[j].link + '">' + adlinks[j].title + '</a> <a href="' + adlinks[k].link + '">' + adlinks[k].title + '</a></li>';
                                    html.push(snippt);
                                }
                                NTES('#js_interst_ad').innerHTML = html.join('');
                            }
                        }, 'utf-8')
                    })();
